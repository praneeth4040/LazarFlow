# Copy this file to .env.local and fill values for development
# Backend API base URL (used by dev server proxy and api client)
VITE_API_BASE_URL=https://lazarflow-backend.onrender.com

# Supabase - provide your Supabase project URL and anon key
VITE_SUPABASE_URL=https://your-supabase-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key

# Optional: Sentry DSN for error tracking
VITE_SENTRY_DSN=
# API Configuration
# Base URL for backend API
# Development: http://localhost:5000
# Production: https://lazarflow-backend.onrender.com
VITE_API_BASE_URL=http://localhost:5000
