{
  "expo": {
    "name": "LazarFlow",
    "slug": "lazarflow-app",
    "owner": "dhaneshvaibhav",
    "scheme": "lazarflow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/padded_icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.lazarflow.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/padded_icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "googleServicesFile": "./google-services.json"
    },
    "notification": {
      "icon": "./assets/padded_icon.png",
      "color": "#ffffff"
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow LazarFlow to save generated table images to your gallery.",
          "savePhotosPermission": "Allow LazarFlow to save generated table images to your gallery."
        }
      ],
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "043f1309-531a-473c-b2a4-dfba407c27d5"
      }
    }
  }
}
